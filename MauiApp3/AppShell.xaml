<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp3"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiApp3">

    <ShellContent
        Title="Ana Sayfa"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Kredi Hesaplama"
        ContentTemplate="{DataTemplate local:KrediHesaplamaPage}"
        Route="Kredi Hesaplama" />
    <ShellContent
     Title="VKI Hesapla"
     ContentTemplate="{DataTemplate local:VKİPage}"
     Route="VKI Hesaplama" />
    <ShellContent
     Title="Renk Seçici"
     ContentTemplate="{DataTemplate local:RenkSeciciPage}"
     Route="Renk Seçici" />

</Shell>
